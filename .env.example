# .env.example
# Copy this file to .env and fill in your actual values

# JWT Secret Key for authentication (generate a random 32-character string)
key=your_jwt_secret_key_here

# Groq API Key for LLM responses (get from https://console.groq.com/keys)
grok=your_groq_api_key_here

# HuggingFace API Tokens for model downloads (get from https://huggingface.co/settings/tokens)
HUGGINGFACE_HUB_TOKEN=your_huggingface_token_here
HF_TOKEN=your_huggingface_token_here